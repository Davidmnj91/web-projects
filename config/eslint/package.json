{
  "name": "@web-projects/eslint",
  "version": "0.0.0",
  "private": true,
  "description": "A package to share ESLint configurations in workspace",
  "author": "David Manja",
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/index.js",
      "types": "./src/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf -g *.tsbuildinfo",
    "lint": "eslint .  --max-warnings 0",
    "lint:fix": "eslint . --fix --max-warnings 0",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@eslint/compat": "catalog:eslint",
    "@eslint/eslintrc": "catalog:eslint",
    "@eslint/js": "catalog:eslint",
    "@next/eslint-plugin-next": "catalog:eslint",
    "eslint-config-prettier": "catalog:eslint",
    "eslint-plugin-import": "catalog:eslint",
    "eslint-plugin-jsx-a11y": "catalog:eslint",
    "eslint-plugin-react": "catalog:eslint",
    "eslint-plugin-react-hooks": "catalog:eslint",
    "eslint-plugin-react-refresh": "catalog:eslint",
    "eslint-plugin-security": "catalog:eslint",
    "eslint-plugin-turbo": "catalog:eslint",
    "globals": "catalog:eslint",
    "typescript-eslint": "catalog:eslint"
  },
  "devDependencies": {
    "@types/eslint-plugin-security": "catalog:eslint",
    "@typescript-eslint/utils": "catalog:eslint",
    "@web-projects/tsconfig": "workspace:*"
  }
}
